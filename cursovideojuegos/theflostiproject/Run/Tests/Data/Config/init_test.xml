<?xml version="1.0" encoding="iso-8859-1"?>
<Config>
  <ScreenResolution width="1024" height="768"/>
  <!--ScreenResolution width="800" height="550"/-->
  <WindowsPosition position="0 0"/>
  <Rendermode fullscreenMode="false"/>
  <Console_Logger activateConsole="true" Log_Level="INFORMATION" activateLogger="true" linesPerPageInLogger="20"/>
  <Mouse exclusiveModeinMouse="true"  drawPointerMouse="true" />
  <ActionToInput fileXML="./Data/actions.xml" />
  <Fonts fontsXML="./Data/Fonts/fonts.xml"/>
  <Languages default="spanish">
    <Language languageXML="./Data/Language/spanish.xml"/>
    <Language languageXML="./Data/Language/catalan.xml"/>
    <Language languageXML="./Data/Language/english.xml"/>
  </Languages>
  <GUI initGuiXML="./Data/GUI/initGui_tests.xml"/>
  <Sound soundXML="./Data/Sounds/Sounds.xml"/>
  <Avi aviXML="./Data/AviFiles/aviFiles.xml"/>
  <PhysX initPhysX="true"/>
  <ComputerVision initComputerVision="false"/>
  <InitScripts>
    <Script action="_core          = getCore();"/>
    <Script action="_inputM        = getCore():getInput();"/>
    <Script action="_renderM       = getCore():getRender();"/>
    <Script action="_physicM       = getCore():getPhysic();"/>
    <Script action="_textureM      = getCore():getTexture();"/>
    <Script action="_effectM       = getCore():getEffect();"/>
    <Script action="_guiM          = getCore():getGui();"/>
    <Script action="_soundM        = getCore():getSound();"/>
    <Script action="_aviPM         = getCore():getAviPlayer();"/>
    <Script action="_networkM      = getCore():getNetwork();"/>
    <Script action="_cvM           = getCore():getComputerVision();"/>
    <Script action="_GprahicTest   = getGrpahicTest();"/>
    <Script action="_aVITest       = getAVITest();"/>
    <Script action="_guiTest       = getGuiTest();"/>
    <Script action="_physXTest     = getPhysXTest();"/>
    <Script action="_spriteTest    = getSpriteTest();"/>
    <Script action="_partSystTest  = getPartSystTest();"/>
  </InitScripts>

  <!--<Process initProcess="AVITest"/>-->
  <!--<Process initProcess="PhysXTest"/>-->
  <!--<Process initProcess="GuiTest"/>-->
  <!--<Process initProcess="SpriteTest"/>-->
  <!--<Process initProcess="ParticleSystemTest"/>-->
  <!--<Process initProcess=""/>-->
  <Process initProcess="AVITest"/>
  
</Config>